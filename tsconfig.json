{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "es6",
    "jsx": "react",
    "jsxFactory": "h",
    "outDir": "build"
  },
  "typeRoots": [
    "node_modules/@types"
  ],
  "exclude": [
    "node_modules"
  ]
}